<% @tab = 'Create' %>

<% content_for :head do %>
  <%= javascript_tag('allClonesPartitionedByMarkerSymbol='+@all_clones_partitioned_by_marker_symbol.to_json) %>
  <%= javascript_include_tag('mi_attempts/shared') %>
  <%= javascript_include_tag('mi_attempts/new') %>
<% end %>

<div>
  <%= form_for @mi_attempt, :html => {:class => 'new mi-attempt'} do |form| %>
    <fieldset>
      <legend>ES cell clone details</legend>

      <div id="clone-selector"></div>
    </fieldset>

    <div id="rest-of-form">
      <%= render 'form_contents', :form => form %>
    </div>
  <% end %>
</div>
