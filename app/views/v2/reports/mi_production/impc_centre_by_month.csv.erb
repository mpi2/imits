<% ImpcCentreByMonthReport.centres.each do |centre| %>

<%= centre %>

Date, Production centre, <%= @es_cell_columns.keys.join(', ') %>, <%= @columns.map {|column, key| ["#{column} (actual)", "#{column} (goal)"] }.flatten.join(', ') %>
<% @centre_by_month[:dates].each do |date| %>
<%= date %>, <%= centre %>, <%= @es_cell_columns.values.map {|column| @centre_by_month["#{date}-#{centre}-#{column}"].to_i}.join(', ') %>, <%= @columns.map {|column, key| [@centre_by_month["#{date}-#{centre}-#{column}"].to_i, @centre_by_month["#{date}-#{centre}-#{column}_goal"].to_i] }.flatten.join(', ') %>
<% end %>
<% end %>