      link_to('Download as CSV', url_for({:action => controller.action_name, :controller => '/reports/mi_production', :format => :csv, :consortium => consortium}.merge(params)))%>    <%=link_to('Download MI image', url_for({:action => 'impc_graph_report_download_image', :controller => '/reports/mi_production', :consortium => consortium.downcase, :goal => 'mi'}.merge(params))) %>    <%= link_to('Download GC image', url_for({:action => 'impc_graph_report_download_image', :controller => '/reports/mi_production', :consortium => consortium.downcase, :goal => 'gc'}.merge(params)))
      %></pre>
      <dt><table height = "670" width = "450">
